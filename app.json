{
  "expo": {
    "name": "sparks",
    "slug": "sparks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366F1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaimegarcia.sparks",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized notifications to you.",
        "NSCameraUsageDescription": "Allow Sparks to access your camera to capture photos of completed challenges.",
        "NSPhotoLibraryUsageDescription": "Allow Sparks to access your photo library to save challenge photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications"
    ],
    "scheme": "sparks",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "197a85c4-64b1-438f-bc1b-888857bba20d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/197a85c4-64b1-438f-bc1b-888857bba20d"
    }
  }
}
